# Copy this to .env and fill in your actual values
# DO NOT commit .env to git!

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5433/apptracker
DATABASE_USER=appuser
DATABASE_PASSWORD=your_secure_password

# Server Port
PORT=8080

# JWT Secret (generate a base64-encoded random string)
# Example: openssl rand -base64 32
JWT_SECRET=your_jwt_secret_here

# Email Configuration (Gmail SMTP)
SUPPORT_EMAIL=your-email@gmail.com
SUPPORT_EMAIL_PASSWORD=your_app_password

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your_account_id
R2_ACCESS_KEY_ID=your_access_key
R2_SECRET_ACCESS_KEY=your_secret_key
R2_BUCKET=your_bucket_name
R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com/your_bucket_name

# OAuth2 - Google
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# OAuth2 - GitHub
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
